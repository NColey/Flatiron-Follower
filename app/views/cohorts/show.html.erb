<h2>Welcome to <%= @cohort.name %></h2>

<div class="row">
  <div class="col-md-1"><%= button_to "Scrape", scrape_cohort_path, :class => "btn btn-default"%></div>
  <div class="col-md-1"><%= button_to "Back", cohorts_path, :method=> "get", :class => "btn btn-default"%></div>
 
</div>
<br>

<table class="table table-striped table-hover">
  <% @cohort.students.each do |student| %>
    <tr><td><%= student.name %></td><td><%= link_to "Edit", edit_admin_student_path(student) %></td></tr>
  <% end %>
</table>